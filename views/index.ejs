<!DOCTYPE html>
<html lang="en">
  <%- include("./partials/head") %>
  <body>
    <%- include("./partials/nav") %>

    <div class="content">
      <h2><%= title %></h2>

      <% if (blogs.length > 0) { %> <% blogs.forEach((blog) => { %>

        <a href="/blog/<%= blog._id %>">
      <div class="blog-item">
        <h3><%= blog.title %></h3>
        <p><%= blog.snippet %></p>
      </div>
      <% }) %> <%} %>
    </div></a>
    <%- include("./partials/footer") %>
  </body>
</html>
